	var pkBaseURL = (("https:" == document.location.protocol) ? "https://stats.rpgorg.net/piwik/" : "http://stats.rpgorg.net/piwik/");
	document.write(unescape("%3Cscript src='" + pkBaseURL + "piwik.js' type='text/javascript'%3E%3C/script%3E"));
</script>
