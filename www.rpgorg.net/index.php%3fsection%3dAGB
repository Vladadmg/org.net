	function SetPositionNewForenDiv() {
		// Get the divs that I need
		var Forum_div = document.getElementById('menuForen');
		var NewForen_div = document.getElementById('NewForenDiv');

		var Obj = Forum_div;   //Get position of "Forum"-Div
		var Pos_Left = 0;
		var Pos_Top = 0;
