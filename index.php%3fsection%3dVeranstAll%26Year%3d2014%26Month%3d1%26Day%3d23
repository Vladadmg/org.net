<script type="text/javascript">
	function Typ_Selected() {		//Hide elements depending on the selected type
		if (document.getElementById('Filter_Typ').value == -1) {
			document.getElementById("Filter_RPG_tabelRow").style.display = "none";				
			document.getElementById("Filter_Spielname").style.display = "none";	
		} else if (document.getElementById('Filter_Typ').value == 0) {  // RPG selected
			document.getElementById("Filter_RPG_tabelRow").style.display = "";
			FadeOpacity ("Filter_RPG_ID_PP", 0.05, 1, 600);
			document.getElementById("Filter_RPG_ID_PP").style.display = "";				
			document.getElementById("Filter_RPG_ID_MMO").style.display = "none";				
			document.getElementById("Filter_RPG_ID_LARP").style.display = "none";				
			document.getElementById("Filter_Spielname").style.display = "none";				
		} else if (document.getElementById('Filter_Typ').value == 10) {  // MMORPG selected
			FadeOpacity ("Filter_RPG_tabelRow", 0.05, 1, 600);
			document.getElementById("Filter_RPG_ID_PP").style.display = "none";				
			FadeOpacity ("Filter_RPG_ID_MMO", 0.05, 1, 600);
			document.getElementById("Filter_RPG_ID_MMO").style.display = "";				
			document.getElementById("Filter_RPG_ID_LARP").style.display = "none";				
			document.getElementById("Filter_Spielname").style.display = "none";				
		} else if (document.getElementById('Filter_Typ').value == 20) {  // LARP selected
			document.getElementById("Filter_RPG_tabelRow").style.display = "";
			document.getElementById("Filter_RPG_ID_PP").style.display = "none";				
			document.getElementById("Filter_RPG_ID_MMO").style.display = "none";
			FadeOpacity ("Filter_RPG_ID_LARP", 0.05, 1, 600);		
			document.getElementById("Filter_RPG_ID_LARP").style.display = "";				
			document.getElementById("Filter_Spielname").style.display = "none";				
		} else {
			document.getElementById("Filter_RPG_tabelRow").style.display = "none";				
			FadeOpacity ("Filter_Spielname", 0.05, 1, 600);
			document.getElementById("Filter_Spielname").style.display = "";				
		}
	}
	
	Typ_Selected();

	function include_ajax(MillisForFullFade) {
		var xmlhttp;
		if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
  			xmlhttp=new XMLHttpRequest();
  		} else {// code for IE6, IE5
  			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  		}
		
		var CountryID = document.getElementById("SelectedCountry").value;
		var km_initial = 0;		// Set initial value for Postleitzahl. If nothing is entered in the currently shown field, use value from database		
		try {
			km_initial = document.getElementById("Filter_km").value;
		} catch(err) {
			km_initial = 0;
		}

		var PLZ_initial = 0;		// Set initial value for Postleitzahl. If nothing is entered in the currently shown field, use value from database		
		try {
			PLZ_initial = document.getElementById("Filter_PLZ").value;
		} catch(err) {
			PLZ_initial = 0;
		}

		var Ort_initial = "";		// Set initial value for Ort. If nothing is entered in the currently shown field, use value from database		
		try {
			Ort_initial = document.getElementById("Filter_Ort").value;
		} catch(err) {
			Ort_initial = "";
		}
		
		var width_td = document.getElementById("td_for_width").offsetWidth - 1;

		var BundeslandID_initial = 0;		// Set initial value for BundeslandID. If nothing is entered in the currently shown field, use value from database		
		try {
			BundeslandID_initial = document.getElementById("Filter_Bundesland").value;
		} catch(err) {
			BundeslandID_initial = 0;
		}

		xmlhttp.onreadystatechange = function() {
			document.getElementById("DependingOnCountry").style.display = 'none';		// hide element if php-page is not jet loaded
	  		if (xmlhttp.readyState==4 && xmlhttp.status==200) {							// when loaded:
				document.getElementById("DependingOnCountry").innerHTML=xmlhttp.responseText;
				FadeOpacity('DependingOnCountry', 0.05, 1, MillisForFullFade);										// Fade the Country depending div in slowly
				document.getElementById("DependingOnCountry").style.display = '';				// Make sure it is visible
				HideAllttt();																	// Hide all ttt because there might be old ttt's displayed that can create bugs
		 	}
		}		
		
		xmlhttp.open("GET","Ajax/VeranstShowAll/PlzOrtBundesl_Selection.php?CountryID="+CountryID+"&SID=lngg6f8lgr22o2cftgk0fjtvu1&km_initial="+km_initial+"&PLZ_initial="+PLZ_initial+"&Ort_initial="+Ort_initial+"&BundeslandID_initial="+BundeslandID_initial+"&width_td="+width_td, true);
		xmlhttp.send();		// load the php-page
	}
	
	include_ajax(10);
</script>



	
	
 
